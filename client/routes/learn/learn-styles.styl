ul.basket
  height: 400px;
  border: 2px dashed #ddd;

ul.free li
  display: inline-block
  margin-left: 10px
  margin-right: 10px

ul.free li:first-child
  margin-left: 0

.title
  color white
  font-size 60px
  margin-top 200px
  margin-bottom 100px

.module
  float none
  display inline-block

.new-module  
  margin: 10px;

.module-header
  color white

.module a
  color white

.module a:hover
  text-decoration none

.slick-prev 
  margin-left -40px

.slick-next
  margin-right -40px

.slick-prev,
.slick-next
  margin-top -40px
  height 60px
  width 60px

.slick-prev:before
  font-size 60px

.slick-next:before
  font-size 60px

.carousel-navigation
  margin-left 35px
  margin-right 40px

.carousel-navigation .slick-next:before,
.carousel-navigation .slick-prev:before
  font-size 40px

.carousel-navigation .slick-prev,
.carousel-navigation .slick-next
  margin-top -25px
  height 40px
  width 40px

.carousel-navigation .slick-prev
  margin-left -15px

.carousel-navigation .slick-next
  margin-right -15px
